  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test map",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/tsp",
      "args": ["./problems/my_test.tsp"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description":  "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ],
      "preLaunchTask": "build"
    },
    {
      "name": "eil76",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/tsp",
      "args": ["./problems/eil76.tsp"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description":  "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ],
      "preLaunchTask": "build"
    }
  ]
}